{
    "appId": "com.vidpro.app",
    "appName": "Video Production App",
    "webDir": "out",
    "bundledWebRuntime": false,
    "server": {
        "url": "http://192.168.1.37:3001/auth/select-role",
        "cleartext": true,
        "allowNavigation": [
            "*"
        ]
    },
    "ios": {
        "contentInset": "automatic"
    },
    "android": {
        "allowMixedContent": true,
        "webContentsDebuggingEnabled": true
    },
    "plugins": {
        "StatusBar": {
            "overlaysWebView": false,
            "backgroundColor": "#000000",
            "style": "DARK"
        },
        "App": {
            "exitOnBackButton": false
        },
        "SplashScreen": {
            "launchAutoHide": true,
            "backgroundColor": "#000000",
            "androidScaleType": "centerInside"
        }
    }
}